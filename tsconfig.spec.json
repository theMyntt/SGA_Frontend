/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": ["jasmine"]
  },
  "baseUrl": "./",
  "paths": {
    "@pages/*": ["src/app/pages/*"],
    "@components/*": ["src/app/components/*"],
    "@guards/*": ["src/app/guards/*"],
    "@interfaces/*": ["src/app/interfaces/*"],
    "@app/*": ["src/app/*"]
  },
  "include": ["src/**/*.spec.ts", "src/**/*.d.ts"]
}
